<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="primaryTextColor" format="reference"/>
    <attr name="buttonNormalColor" format="reference"/>
    <attr name="buttonHighlightColor" format="reference" />
    <attr name="buttonTextColor" format="reference" />
    <attr name="iconColor" format="reference" />

    <attr name="textViewStyle" format="reference" />
    <attr name="primaryButtonTheme" format="reference" />

    <attr name="bg_1" format="reference" />

    <attr name="bg_2" format="reference" />

    <attr name="bg_list_1" format="reference" />

    <attr name="bg_list_2" format="reference" />

    <attr name="bg_home_1" format="reference" />

    <attr name="bg_home_2" format="reference" />

    <attr name="bg_detail_1" format="reference" />

    <attr name="bg_detail_2" format="reference" />

    <attr name="bg_rcv_item_1" format="reference" />
    <attr name="bg_rcv_item_2" format="reference" />


    <attr name="bg_card_1" format="reference" />
    <attr name="bg_card_2" format="reference" />

    <attr name="bg_fab_1" format="reference" />

    <attr name="bg_view_1" format="reference" />
    <attr name="bg_view_2" format="reference" />


    <attr name="bg_border_1" format="reference" />
    <attr name="bg_border_2" format="reference" />


    <attr name="text_color_1" format="reference"/>
    <attr name="text_color_2" format="reference"/>
    <attr name="text_color_3" format="reference"/>
    <attr name="text_color_4" format="reference"/>
    <attr name="text_color_5" format="reference"/>





    <attr name="btn_text_color_1" format="reference"/>
    <attr name="btn_text_color_2" format="reference"/>


    <attr name="tint_1" format="reference" />
    <attr name="tint_2" format="reference" />

    <attr name="gradient_1" format="reference" />
    <attr name="gradient_2" format="reference" />
    <attr name="gradient_3" format="reference" />
    <attr name="gradient_4" format="reference" />

    <declare-styleable name="DeviceFitImageView">
        <attr name="width_percentage" format="integer"/>
        <attr name="tolerance_width" format="integer"/>
        <attr name="height_percentage" format="integer"/>
        <attr name="tolerance_height" format="integer"/>
        <attr name="blur_radius" format="integer"/>
        <attr name="aspect_ratio" format="float" />
        <attr name="parameter">
            <enum name="aspect_width" value="0"/>
            <enum name="aspect_height" value="1"/>
            <enum name="fixed_ratio" value="2"/>
            <enum name="measure_width" value="3"/>
            <enum name="measure_height" value="4"/>
            <enum name="square" value="5"/>
        </attr>
    </declare-styleable>
    <!-- styleable attributes for cardiogram values view -->
    <declare-styleable name="ImagePickView">
        <attr name="show_close_icon" format="boolean" />
        <attr name="add_image_mode" format="boolean" />
        <attr name="levelUnit" format="string" />
    </declare-styleable>

    <declare-styleable name="CurveView">
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />

        <attr name="startColorX" format="float" />
        <attr name="startColorY" format="float" />
        <attr name="endColorX" format="float" />
        <attr name="endColorY" format="float" />

        <attr name="firstControlPointX" format="float" />
        <attr name="firstControlPointY" format="float" />
        <attr name="secondControlPointX" format="float" />
        <attr name="secondControlPointY" format="float" />

        <attr name="curveShadowRadius" format="dimension"/>
        <attr name="curveShadowColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="GradientChart">

        <!-- chart gradient colors -->
        <attr name="chartLineColor" format="color"/>
        <attr name="plusColorStart" format="color"/>
        <attr name="plusColorEnd" format="color"/>
        <attr name="minusColorStart" format="color"/>
        <attr name="minusColorEnd" format="color"/>

        <!-- other -->
        <attr name="isBezier" format="boolean"/>
        <attr name="bezierIntensity" format="float"/>
        <attr name="zoom" format="integer"/>

    </declare-styleable>
</resources>